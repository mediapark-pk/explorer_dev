{
  "name": "ddk.explorer-2-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "watch": "nest build --webpack --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "prod": "npx ts-node src/main.ts",
    "dev": "node --inspect dist/main.js",
    "build:dev": "nest build --webpack",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/modules/database/options.ts",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:run": "npm run typeorm migration:run",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@app/common": "file:../packages/common",
    "@app/socket-nest": "file:../packages/socket-nest",
    "@app/socket-types": "file:../packages/socket-types",
    "@app/web": "file:../packages/web",
    "@nestjs-plus/discovery": "^2.0.2",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/platform-ws": "^6.10.5",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjs/websockets": "^6.10.5",
    "@types/webpack-env": "^1.15.0",
    "cache-manager": "^2.10.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "factory.ts": "^0.5.1",
    "geoip-lite": "^1.4.0",
    "module-alias": "^2.2.2",
    "pg": "^7.14.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "ts-node": "^8.5.4",
    "typeorm": "^0.2.21",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.6",
    "@types/ws": "^6.0.4",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2",
    "source-map-loader": "^0.2.3",
    "rimraf": "^3.0.0"
  },
