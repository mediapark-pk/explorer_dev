{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "member-ordering": [
      false
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "indent": [
      true,
      "spaces",
      4
    ],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^\/\/ .*"
      }
    ],
    "max-classes-per-file": false,
    "no-empty": false,
    "no-shadowed-variable": true,
    "no-switch-case-fall-through": true,
    "trailing-comma": true,
    "no-unused-expression": [
      true,
      "allow-fast-null-checks"
    ],
    "no-unused-variable": false,
    "quotemark": [
      true,
      "single",
      "jsx-single"
    ],
    "no-duplicate-switch-case": true,
    "complexity": [
      "error",
      4
    ],
    "switch-default": true,
    "prefer-object-spread": true,
    "no-magic-numbers": true,
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  },
  "rulesDirectory": []
}